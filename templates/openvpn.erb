[connection]
id=<%= @id %>
uuid=<%= @uuid %>
type=vpn
permissions=user:<%= @user %>:;
autoconnect=<%= @autoconnect %>

[vpn]
service-type=org.freedesktop.NetworkManager.openvpn
ta-dir=<%= @ta_dir %>
connection-type=<%= @connection_type %>
password-flags=<%= @password_flags %>
remote=<%= @remote %>
comp-lzo=<%= @comp_lzo %>
username=<%= @user %>
ca=<%= @ca %>
ta=<%= @ta %>

[ipv4]
<% if @routes -%>
routes1=<%= @routes %>
<% end -%>
method=<%= @ipv4_method %>
never-default=<%= @never_default %>
